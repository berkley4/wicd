name = PEAP with GTC
author = Adam Blackburn
version = 3
require identity *Identity password *Password
protected password *Password
optional ca_cert *Path_to_CA_Cert
-----
ctrl_interface=/var/run/wpa_supplicant
network={
	ssid="$_ESSID"
	proto=RSN
	key_mgmt=WPA-EAP
	pairwise=CCMP
	eap=PEAP
	identity="$_IDENTITY"
	password="$_PASSWORD"
	ca_cert="$_CA_CERT"
}
